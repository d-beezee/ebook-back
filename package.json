{
  "name": "ebookz-back",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "dependencies": {
    "canvas": "^2.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs": "^0.0.1-security",
    "pdf-to-base64": "^1.0.3",
    "pdfjs": "^2.4.7",
    "pdfjs-dist": "^2.13.216",
    "sequelize": "^6.17.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/sequelize": "^4.28.11",
    "@types/sqlite3": "^3.1.8",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.6.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "build": "tsc --noEmit false --outDir dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon ",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:new": "sequelize-cli migration:create --name ",
    "model:new": "sequelize-cli model:generate --name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maithanhdanh/express-typescript-boilerplate.git"
